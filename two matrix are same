#include<iostream>

using namespace std;

int main()
{
    int n,m;
    cin>>n>>m;
    int a[n][n],b[m][m];
    cout<<"first";
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>a[i][j];
            
        }
    }
    cout<<"second";
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<m;j++)
        {
            cin>>b[i][j];
        }
    }
    if(m!=n)
    {
    cout<<"change matrix";    
    }
    else
    {
         for(int i=0;i<m;i++)
    {
        for(int j=0;j<m;j++)
        {
            if(a[i][j]==b[i][j])
            {
                cout<<"both of them are same";
            }
        }
    }
    }
    
}
